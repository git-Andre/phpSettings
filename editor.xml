<application>
  <component name="CodeFoldingSettings">
    <option name="COLLAPSE_DOC_COMMENTS" value="true" />
  </component>
  <component name="CodeInsightSettings">
    <option name="SHOW_FULL_SIGNATURES_IN_PARAMETER_INFO" value="true" />
  </component>
  <component name="DaemonCodeAnalyzerSettings">
    <option name="NEXT_ERROR_ACTION_GOES_TO_ERRORS_FIRST" value="false" />
  </component>
  <component name="DefaultFont">
    <option name="FONT_SIZE" value="14" />
    <option name="FONT_FAMILY" value="Fira Code" />
    <option name="FONT_REGULAR_SUB_FAMILY" value="Medium" />
    <option name="FONT_BOLD_SUB_FAMILY" value="Medium" />
    <option name="SECONDARY_FONT_FAMILY" value="JetBrains Mono NL" />
  </component>
  <component name="InlayHintsSettings">
    <option name="lastViewedProviderKeyId" value="docker.inlay.hints" />
  </component>
  <component name="PhpCodeFoldingSettings">
    <option name="COLLAPSE_FUNCTION_BODY" value="true" />
    <option name="COLLAPSE_METHOD_BODY" value="true" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
    <pattern icon="question" useCustomColors="true" pattern="\breview\b.*">
      <option name="FOREGROUND" value="c0be06" />
      <option name="FONT_TYPE" value="2" />
      <option name="EFFECT_COLOR" value="3c3f41" />
      <option name="ERROR_STRIPE_COLOR" value="977ab" />
      <option name="EFFECT_TYPE" value="2" />
    </pattern>
    <pattern pattern="\bverify\.*" />
    <filter name="[\bMaarten\b\]">
      <pattern index="3" />
    </filter>
  </component>
</application>