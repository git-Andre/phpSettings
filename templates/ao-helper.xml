<templateSet group="ao-helper">
  <template name="++dd" value="dd($end$);" shortcut="ENTER" description="dd dump" toReformat="false" toShortenFQNames="true">
    <variable name="end" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="HTML" value="true" />
      <option name="Twig" value="true" />
      <option name="TypeScript" value="true" />
    </context>
  </template>
  <template name="++ts" value="    templateUrl: './$component$.html',&#10;    styleUrls  : ['./$component$.css'],&#10;" description="require helper" toReformat="false" toShortenFQNames="true">
    <variable name="component" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="TypeScript" value="true" />
    </context>
  </template>
  <template name="++cl" value="console.log('$end$: ' + $end$);" description="console.log" toReformat="true" toShortenFQNames="true">
    <variable name="end" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="TypeScript" value="true" />
    </context>
  </template>
  <template name="++js" value="JSON.stringify($end$)" description="JSON.stringify" toReformat="false" toShortenFQNames="true">
    <variable name="end" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JSON" value="true" />
      <option name="TypeScript" value="true" />
    </context>
  </template>
  <template name="++cd" value="console.log($end$);" description="console.log" toReformat="false" toShortenFQNames="true">
    <variable name="end" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JSON" value="true" />
      <option name="TypeScript" value="true" />
    </context>
  </template>
</templateSet>