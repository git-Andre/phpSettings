<templateSet group="ao-helper">
  <template name="++cf" value="db.fInfo( &quot;$clipField$ : &quot; + $clip$ );" description="Console Field Infos" toReformat="true" toShortenFQNames="true">
    <variable name="clipField" expression="clipboard()" defaultValue="" alwaysStopAt="true" />
    <variable name="clip" expression="clipboard()" defaultValue="" alwaysStopAt="false" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="++co" value="console.println(&quot;$Text$ : &quot; + $clip$);" description="console.println" toReformat="true" toShortenFQNames="true">
    <variable name="Text" expression="classNameComplete()" defaultValue="" alwaysStopAt="true" />
    <variable name="clip" expression="clipboard()" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="++cv" value="db.cV( &quot;$Nr$: $file$ -&gt; $clipText$ --&gt; &quot;, $clip$ );" description="debug Variable Infos" toReformat="true" toShortenFQNames="true">
    <variable name="Nr" expression="lineNumber()" defaultValue="" alwaysStopAt="false" />
    <variable name="file" expression="fileName()" defaultValue="" alwaysStopAt="false" />
    <variable name="clipText" expression="clipboard()" defaultValue="" alwaysStopAt="true" />
    <variable name="clip" expression="clipboard()" defaultValue="" alwaysStopAt="false" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="++c" value="console.println(&quot;$cursor$ : &quot;);" description="Console nur Text" toReformat="true" toShortenFQNames="true">
    <variable name="cursor" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="++msg" value="xfa.host.messageBox(&#10;        &quot;$cursor$&quot;, &quot;Bitte überprüfen...&quot;, 0, 0 );" description="" toReformat="true" toShortenFQNames="true">
    <variable name="cursor" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="++if" value="if ($condition$) {&#10;    $block$&#10; } else {&#10;    $elseBlock$&#10; }" description="if Block" toReformat="false" toShortenFQNames="true">
    <variable name="condition" expression="clipboard()" defaultValue="" alwaysStopAt="true" />
    <variable name="block" expression="" defaultValue="// block" alwaysStopAt="true" />
    <variable name="elseBlock" expression="" defaultValue="// else Block" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="++cn" value="db.listObjectNodes($oKnode$);" description="display all Nodes" toReformat="true" toShortenFQNames="true">
    <variable name="oKnode" expression="clipboard()" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
</templateSet>