<templateSet group="ao-helper">
  <template name="++dd" value="dd($end$);" shortcut="ENTER" description="dd dump" toReformat="false" toShortenFQNames="true">
    <variable name="end" expression="" defaultValue="" alwaysStopAt="true" />
  </template>
  <template name="++con" value="console.println(&quot;$clip$ : &quot; + $clip$);" description="console.println" toReformat="true" toShortenFQNames="true">
    <variable name="clip" expression="clipboard()" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
</templateSet>